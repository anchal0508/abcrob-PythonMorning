a = [21,12,33,44,65,76,998]

print(a)

print("Length of a: ",len(a))

print("a[:3]", a[:3])

print("a[2:]\t\t",a[2:])

print("a[:]\t\t",a[:])

print("a[4:1:-1]\t",a[4:1:-1])

print("a[0]\t",a[0])

print("a[1]\t",a[1])

print("a[2]\t",a[2])

print("a[3]\t",a[3])



print("a[6]\t",a[6])



for i in a:
    print(i,end=" ")
